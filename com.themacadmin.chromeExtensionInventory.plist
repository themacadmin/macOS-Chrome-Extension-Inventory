"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
     <key>Label</key>
     <string>com.themacadmin.chromeExtensionInventory</string>
     <key>ProgramArguments</key>
     <array>
          <string>/Library/myOrg/Scripts/chromeExtensionInventory.bash</string>
     </array>
